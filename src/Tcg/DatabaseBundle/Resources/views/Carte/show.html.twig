{% extends "TcgDatabaseBundle::layout.html.twig" %}

{% block content %}
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Typecarte</th>
                <td>{{ entity.typeCarte }}</td>
            </tr>
            <tr>
                <th>Donnees</th>
                <td>{{ entity.donnees }}</td>
            </tr>
            <tr>
                <th>Caracteristiques</th>
                <td>{{ entity.caracteristiques }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ entity.image }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('carte_list') }}">Retour</a>
    <a href="{{ path('carte_edit', { 'id': entity.id }) }}">Modifier</a>
    <a href="{{ path('carte_delete', { 'id': entity.id }) }}">Supprimer</a>
{% endblock %}