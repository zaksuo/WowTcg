<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="donnees_list" pattern="/donnees">
        <default key="_controller">TcgDatabaseBundle:TcgDonnees:index</default>
    </route>

    <route id="donnees_show" pattern="/donnees/{id}/show">
        <default key="_controller">TcgDatabaseBundle:TcgDonnees:show</default>
    </route>

    <route id="donnees_new" pattern="/donnees/new">
        <default key="_controller">TcgDatabaseBundle:TcgDonnees:new</default>
    </route>

    <route id="donnees_create" pattern="/donnees/create">
        <default key="_controller">TcgDatabaseBundle:TcgDonnees:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="donnees_edit" pattern="/donnees/{id}/edit">
        <default key="_controller">TcgDatabaseBundle:TcgDonnees:edit</default>
    </route>

    <route id="donnees_update" pattern="/donnees/{id}/update">
        <default key="_controller">TcgDatabaseBundle:TcgDonnees:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="donnees_delete" pattern="/donnees/{id}/delete">
        <default key="_controller">TcgDatabaseBundle:TcgDonnees:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
